print('-------------------------------------')

# Question 25
print('--- Question 25: Mixed Type Calculation (a + b + int(c)) ---')
a = 5
b = 2.5
c = '3'

c_int = int(c)

result25 = a + b + c_int

print('Variable a:', a, ', Type:', type(a))
print('Variable b:', b, ', Type:', type(b))
print('Variable c (original):', c, ', Type:', type(c))
print('Variable c (converted):', c_int, ', Type:', type(c_int))
print('Result of a + b + int(c):', result25)
print('-------------------------------------')

# Question 26
print('--- Question 26: Boolean Operator Precedence ---')
x = True
y = False
z = True

result26A = x or y and z
print('x or y and z:', result26A)

result26B = (x or y) and z
print('(x or y) and z:', result26B)

print('-------------------------------------')

# Question 27
print('--- Question 27: Step-by-Step Operator Precedence (10 + 2 * 3 ** 2 // 4 - 5) ---')

val1 = 10 + 2 * 9 // 4 - 5
print('After **:', val1)

val2 = 10 + 18 // 4 - 5
print('After *:', val2)

val3 = 10 + 4 - 5
print('After //:', val3)

val4 = 14 - 5
print('After +:', val4)

final_result27 = 9
print('After -:', final_result27)
print('-------------------------------------')

# Question 28
print('--- Question 28: Single Input to Different Types ---')
user_input = input('Enter a single value: ')

try:
    int_val = int(user_input)
except ValueError:
    int_val = 'N/A (Invalid for int)'

float_val = float(user_input)

str_val = str(user_input)

print('As Integer:', int_val, ', Type:', type(int_val) if int_val != 'N/A (Invalid for int)' else type(user_input))
print('As Float:', float_val, ', Type:', type(float_val))
print('As String:', str_val, ', Type:', type(str_val))
print('-------------------------------------')

# Question 29
print('--- Question 29: Parentheses Effect on Evaluation (100 / 5 * 2 + 10 - 3 ** 2) ---')

original_result = (100 / 5 * 2 + 10 - 3 ** 2)
print('Original Result:', original_result)

result = 100 / (5 * 2) + 10 - 3 ** 2
print('Rewrite 1 (100 / (5 * 2) + 10 - 3 ** 2):', result)

result29B = 100 / 5 * 2 + (10 - 3) ** 2
print('Rewrite 2 (100 / 5 * 2 + (10 - 3) ** 2):', result)

print('-------------------------------------')

# Question 30
print('--- Question 30: Algebraic Identity Verification ---')
a30 = float(input('Enter number a: '))
b30 = float(input('Enter number b: '))

lhs_term1 = (a30 + b30) ** 2
lhs_term2 = (a30 - b30) ** 2
lhs = lhs_term1 - lhs_term2
print('(a + b) ** 2:', lhs_term1)
print('(a - b) ** 2:', lhs_term2)
print('LHS ((a + b) ** 2 - (a - b) ** 2):', lhs)

rhs = 4 * a30 * b30
print('RHS (4 * a * b):', rhs)

verification = (lhs == rhs)
print('Verification (LHS == RHS):', verification)
